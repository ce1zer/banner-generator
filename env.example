NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Comma-separated admin allowlist (emails). Example: "you@example.com,other@example.com"
ADMIN_EMAILS=

# Nano Banana Pro (server-side only). If missing, app uses a placeholder image generator locally.
NANO_BANANA_API_URL=
NANO_BANANA_API_KEY=

# Nano Banana mapping (keeps adapter clean without assuming provider schema)
# Provide a JSON request body template and use placeholders:
# - {{PROMPT}}
# - {{REFERENCE_IMAGE_URL}}
# - {{ASPECT}}  (always "4:5")
NANO_BANANA_REQUEST_TEMPLATE_JSON=

# Optional auth header/value customizations (defaults to Authorization: Bearer {{API_KEY}})
NANO_BANANA_AUTH_HEADER=
NANO_BANANA_AUTH_VALUE_TEMPLATE=

# Optional response parsing hints:
# - auto (default): tries image/*, then JSON base64, then JSON url
# - image: expects raw image bytes
# - json_base64: expects JSON with base64 field
# - json_url: expects JSON with image url field
NANO_BANANA_RESPONSE_MODE=
NANO_BANANA_JSON_IMAGE_BASE64_FIELD=
NANO_BANANA_JSON_IMAGE_URL_FIELD=
NANO_BANANA_JSON_CONTENT_TYPE_FIELD=

# Debug logging (server-side). If set to 1/true, logs response shape (not prompts/keys).
NANO_BANANA_DEBUG=

